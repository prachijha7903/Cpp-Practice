#include <iostream>
using namespace std;

int main() {
    int arr[5] = {3, 10, 7, 5, 1};
    int key = 10;
    bool found = false;

  for(int i = 0; i < 5; i++) {
        if(arr[i] == key) {
            cout << "10 is found at index " << i;
            found = true;
            break;
        }
    }

  if(!found) {
        cout << "10 is not found in the array";
    }

   return 0;
}
