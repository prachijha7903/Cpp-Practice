#include<iostream>
using namespace std ;

class A{
    int x;
    public:
    void input(int y){
        x = y ;
    }
    A operator+(A ob){
        A a;
        a.x = x + ob.x ;
        return a;
    }
    void show(){
        cout<<x;
    }
} ;
int main(){
    A ob1,ob2,ob3;
    ob1.input(5);
    ob2.input(6);

    ob3 = ob1 + ob2 ;
    ob3.show();
    return 0;
}
