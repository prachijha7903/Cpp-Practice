#include<iostream>
using namespace std ;

int main(){
    int size ;
    cout<<"Enter the size of array : ";
    cin>>size;

  int price[size];
    for(int i = 0 ; i <= size-1 ;i++){
        cout<<"Enter "<<i <<" index number : ";
        cin>>price[i];
    }
    for(int i = 0 ; i <= size-1 ; i++){
        cout<<"The price at index "<<i <<" is : " <<price[i] <<endl;
    }
    return 0;
}
