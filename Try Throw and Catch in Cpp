#include<iostream>
using namespace std;

int main(){
    int a , b ;
    cout<<"Enter the numbers :";
    cin>>a>>b;
     
  try{
        if(b == 0)
        throw b;
        cout<<a<<"/"<<b<<"is: "<<(a/b);

    }
   catch(int e){
        cout<<"Exception is there "<<e<<endl;
    }
    cout<<"Execution continues";

   return 0;
}
