#include <iostream>
#include <map>
using namespace std;

int main() {
    map<string, int> phoneBook;
    int n;

    cout << "Enter number of contacts: ";
    cin >> n;

    // Adding contacts
    for (int i = 0; i < n; i++) {
        string name;
        long long number;

        cout << "Enter name: ";
        cin >> name;
        cout << "Enter phone number: ";
        cin >> number;

        phoneBook[name] = number;
    }

    // Searching contact
    string searchName;
    cout << "\nEnter name to search: ";
    cin >> searchName;

    auto it = phoneBook.find(searchName);

    if (it != phoneBook.end()) {
        cout << "Contact Found!\n";
        cout << "Name: " << it->first << endl;
        cout << "Phone Number: " << it->second << endl;
    } else {
        cout << "Contact not found!";
    }

    return 0;
}
