#include<iostream>
using namespace std ;

class A{
    int roll;
    public:
    A(int a){
       roll = a ;
    }
    A(const A &ob){
       roll = ob.roll ;
    }
    void display(){
        cout<<"Roll = "<<roll<<endl ;
    }
};

int main(){
   A ob1(10);
   A ob2(ob1);

  ob1.display();
  ob2.display();
  return 0;
}
