#include<iostream>        
using namespace std;

int main(){
    int n ;
    cout<<"Enter the size of Array : ";
    cin>>n;

  int *ptr = new int[n];    

  for(int i = 0 ; i<n ; i++){
        cout<<"Enter the Array at index "<<i;
        cin>>*(ptr+i);
    }
    int sum = 0 ;
    for(int i = 0 ; i<n ; i++){
        sum = sum + *(ptr+i);
    }
    cout<<sum;
    delete[] ptr ;
    return 0;
}  
