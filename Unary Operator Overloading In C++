#include<iostream>
using namespace std;

class first{
    int x,y;
    public:
    void input(){
        cout<<"Enter x = ";
        cin>>x;
        cout<<"Enter y = ";
        cin>>y;
    }
    void output(){
        cout<<x <<endl<<y;
    }
    void operator++(){
        x = x + 1 ;
        y = y + 5 ;
    }
};

int main(){
    first f;
    f.input();
    ++f;
    f.output();
    return 0;
}
