#include <iostream>
using namespace std;

int main() {
    int arr[5];
    int smallest;

  // Input array elements
    cout << "Enter 5 numbers:\n";
    for(int i = 0; i < 5; i++) {
        cin >> arr[i];
    }
    
  smallest = arr[0] ;

  for(int i = 1; i < 5; i++) {
        if(arr[i] < smallest) {
            smallest = arr[i];
        }
    }
    
  cout << "Smallest number in the array is: " << smallest;

   return 0;
}
