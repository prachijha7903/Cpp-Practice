#include<stdio.h>                           
int main(){
    char day;
    printf("ENter the day: ");
    scanf("%c",&day);

  switch(day){
        case 'm' : printf("Monday");
                   break;
        case 't' : printf("Tuesday");
                   break;
        case 'w' : printf("Wednesday");
        case 'T' : printf("Thursday");
        case 'f': printf("Friday");
        case 's' : printf("Saturday");
        case 'S': printf("sunday");
        default :printf("Not a valid date");
    }

  return 0;
}
