#include <iostream>
#include <queue>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = 5;

  queue<int> q;

  // Push all elements into the queue
    for(int i = 0; i < n; i++) {
        q.push(arr[i]);
    }

  // Pop from queue (FIFO order)
    cout << "Queue output order: ";
    while(!q.empty()) {
        cout << q.front() << " ";  // front() gives first inserted element
        q.pop();                   // removes the element
    }
return 0;
}
