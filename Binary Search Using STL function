#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int arr[] = {2, 4, 6, 8, 10};   // Sorted array
    int n = 5;
    int key = 10;

  // STL binary_search
    if (binary_search(arr, arr + n, key)) {
        cout << "Element " << key << " is found in the array";
    } else {
        cout << "Element " << key << " is not found in the array";
    }

   return 0;
}
