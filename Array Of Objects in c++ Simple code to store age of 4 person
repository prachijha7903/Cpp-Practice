#include<iostream>
using namespace std;

class demo{
public:
    int age[4];

  void input(){
        for(int i = 0 ; i < 4 ; i++){
            cout << "Enter age: ";
            cin >> age[i];
        }
    }
void output(){
        for(int i = 0 ; i < 4 ; i++){
            cout << "The Person age at index " << i << " is " << age[i] << endl;
        }
    }
};

int main(){
    demo d[4];  // create array of 4 objects
    
   d[0].input();   // input values for first object
    d[0].output();  // print values

  return 0;
}
