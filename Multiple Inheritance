#include <iostream>
using namespace std;

class Father {
public:
    void house() {
        cout << "Father's house\n";
    }
};

class Mother {
public:
    void jewelry() {
        cout << "Mother's jewelry\n";
    }
};

class Child : public Father, public Mother {
public:
    void bike() {
        cout << "Child's bike\n";
    }
};

int main() {
    Child c;
    c.house();
    c.jewelry();
    c.bike();

    return 0;
}
