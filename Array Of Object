#include <iostream>
using namespace std;

class Student {
public:
    int marks; 
};

int main() {
    Student s[3]; // array of 3 Student objects

    // Input marks for each student
  for(int i = 0; i < 3; i++) {
        cout << "Enter marks of student " << i << ": ";
        cin >> s[i].marks;
    }

  cout << "\nDisplaying Marks:\n";
    for(int i = 0; i < 3; i++) {
        cout << "Marks of student " << i << " = " << s[i].marks << endl;
    }

   return 0;
}
