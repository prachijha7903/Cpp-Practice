#include<iostream>
using namespace std ;

int main(){
    int n ;
    cout<<"Enter total no of elements of array or Size of Array : " ;
    cin>>n;
    int *ptr = new int [n];
    cout<<"Enter elements : "<<endl;

  for(int i = 0 ; i < n ; i++){
        cout<<"Enter array at index "<<i <<" : ";
        cin>>*(ptr + i);
  }
  
  for(int i = 0 ; i < n ; i++){
        cout<<*(ptr + i);
  }
return 0;
}
