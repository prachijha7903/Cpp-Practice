#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int arr[] = {2, 4, 6, 8, 10};
    int n = 5;
    int key = 6;

    int* pos = lower_bound(arr, arr + n, key);

    if (pos != arr + n && *pos == key) {
        cout << "Element found at index " << (pos - arr);
    } else {
        cout << "Element not found";
    }
    return 0;
}
