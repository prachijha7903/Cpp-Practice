#include<iostream>
#include<string>
using namespace std ;
 
class University{
    public:
    string name;
    string location;
    void input1(){
        cout<<"Enter Name= ";
        cin>>name;
        cout<<"Enter Location = ";
        cin>>location;
   
    }
    void display1(){
        cout<<"\n";
        cout<<"Name ="<<name;
        cout<<"Location ="<<location;
    }
   
};
class College :public University{
    public:
    string college_name ;
    int code;
    void input2(){
        cout<<"Enter college name : ";
        cin>>college_name;
        cout<<"Enter code : ";
        cin>>code;
    }
    void display2(){
        cout<<"The name of college is : "<<college_name ;
        cout<<"code is: "<<code;
}
};
class Depart :public College{
    public:
    string de_name ;
    int NoOfStudent;
    void input3(){
        cout<<"Enter depart name : ";
        cin>>de_name;
        cout<<"Enter No of student : ";
        cin>>NoOfStudent;
    }
    void display3(){
        cout<<"The name of college is : "<<college_name ;
        cout<<"NoOfStudent: "<<NoOfStudent;
}
};
int main(){
    Depart ob;
    ob.input1();
    ob.input2();
    ob.input3();

    ob.display1();
    ob.display2();
    ob.display3();

   
    return 0;
}
