#include <iostream>
#include <stack>
using namespace std;

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int n = 5;

  stack<int> s;

  // Push all elements into the stack
    for(int i = 0; i < n; i++) {
        s.push(arr[i]);
    }

  // Pop from stack (LIFO order)
    cout << "Stack output order: ";
    while(!s.empty()) {
        cout << s.top() << " ";  // top() gives last inserted element
        s.pop();                 // removes the element
    }
return 0;
}
